{
  "name": "electron-with-reactjs",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^6.0.12",
    "electron-is-dev": "^1.1.0",
    "moment": "^2.24.0",
    "pikaday": "^1.8.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-paginate": "^6.3.0",
    "react-scripts": "3.2.0",
    "wait-on": "^3.3.0"
  },
  "scripts": {
